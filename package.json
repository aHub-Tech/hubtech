{
  "name": "nextjs-pro-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "generate": "npx plop -- --plopfile generators/plopfile.js",
    "storybook": "start-storybook --no-manager-cache -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "src/**/*": [
      "npm run lint --fix",
      "npm run test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "next": "^11.1.2",
    "next-pwa": "^5.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/preset-typescript": "^7.12.17",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.30",
    "@types/react": "^17.0.2",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.1.1",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "lint-staged": "^10.5.4",
    "plop": "^2.7.4",
    "prettier": "^2.2.1",
    "storybook-addon-next-router": "^3.0.8",
    "typescript": "^4.1.5"
  }
}
